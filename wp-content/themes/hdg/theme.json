{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
	  "appearanceTools": true,
	  "color": {
		"custom": false,
		"customDuotone": false,
		"customGradient": false,
		"defaultDuotone": true,
		"defaultGradients": false,
		"defaultPalette": false,
		"palette": [
		  {
			"name": "Primary",
			"slug": "primary",
			"color": "#3C3C3C"
		  },
		  {
			"name": "Secondary",
			"slug": "secondary",
			"color": "#F5F5F5"
		  },
		  {
			"name": "Green",
			"slug": "green",
			"color": "#4d8b74"
		  },
		  {
			"name": "Highlight",
			"slug": "highlight",
			"color": "#6025a3"
		  },
		  {
			"name": "Pink",
			"slug": "pink",
			"color": "#d1b3e5"
		  },
		  {
			"name": "Blue",
			"slug": "blue",
			"color": "#4b4cee"
		  },
		  {
			"name": "Dark",
			"slug": "dark",
			"color": "#0d0f05"
		  },
		  {
			"name": "Light",
			"slug": "light",
			"color": "#ffffff"
		  },
		  {
			"name": "Focus",
			"slug": "focus",
			"color": "#FFDD00"
		  }
		]
	  },
	  "layout": {
		"contentSize": "1120px"
	  },
	  "spacing": {
		"defaultFontSizes": false,
		"defaultSpacingSizes": false,
		"spacingScale": {
		  "steps": 0
		},
		"spacingSizes": [
		  {
			"name": "XS",
			"size": "var(--wp--custom--spacing--xs)",
			"slug": "x-small"
		  },
		  {
			"name": "Small",
			"size": "var(--wp--custom--spacing--sm)",
			"slug": "small"
		  },
		  {
			"name": "Medium",
			"size": "var(--wp--custom--spacing--md)",
			"slug": "medium"
		  },
		  {
			"name": "Large",
			"size": "var(--wp--custom--spacing--lg)",
			"slug": "large"
		  },
		  {
			"name": "XL",
			"size": "var(--wp--custom--spacing--xl)",
			"slug": "x-large"
		  }
		],
		"units": [
		  "px",
		  "em",
		  "rem",
		  "vh",
		  "vw",
		  "%"
		]
	  },
	  "typography": {
		"customFontSize": false,
		"fontStyle": false,
		"fontWeight": false,
		"letterSpacing": false,
		"lineHeight": false,
		"textDecoration": false,
		"textTransform": false,
		"dropCap": false,
		"fluid": true,
		"fontFamilies": [
		  {
			"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
			"name": "System font",
			"slug": "system-font"
		  },
		  {
			"fontFamily": "Raleway, sans-serif",
			"name": "Raleway",
			"slug": "body"
		  },
		  {
			"fontFamily": "monospace",
			"name": "Monospace",
			"slug": "monospace"
		  }
		],
		"fontSizes": [
		{
				"name": "Extra Small",
				"size": "0.9rem",
				"fluid": false,
				"slug": "xs"
		 },
		  {
			"name": "Small",
			"size": "1rem",
			"fluid": false,
			"slug": "sm"
		  },
		  {
			"name": "Body",
			"size": "1.15rem",
			"fluid": false,
			"slug": "body"
		  },
		  {
			"name": "Medium",
			"size": "1.5rem",
			"slug": "md", 
			"fluid": {
				"min": "1rem",
				"max": "1.5rem"
			}
		  },
		  {
			"name": "Large",
			"size": "2rem",
			"slug": "lg",
			"fluid": {
				"min": "1.5rem",
				"max": "2rem"
			}
		  },
		  {
			"name": "Extra Large",
			"size": "3rem",
			"slug": "xl",
			"fluid": {
				"min": "2.5rem",
				"max": "3rem"
			}
		  },
		  {
			"name": "2XL",
			"size": "3.4rem",
			"slug": "2xl",
			"fluid": {
				"min": "3rem",
				"max": "3.4rem"
			}
		  }
		]
	  },
	  "useRootPaddingAwareAlignments": true,
	  "custom": {
		"spacing": {
		  "gap": "1.25rem",
		  "2xs": "0.25rem",
		  "xs": "0.5rem",
		  "sm": "0.75rem",
		  "md": "0.9375rem",
		  "lg": "2rem",
		  "xl": "3rem",
		  "2xl": "4rem",
		  "s-xxl": "clamp(1.0625rem, -0.0053rem + 5.339vi, 5rem)"
		},
		"typography": {
		  "letterSpacing": {
			"body": "0",
			"heading": "0.02em"
		  },
		  "lineHeight": {
			"body": "1.333333",
			"heading": "1"
		  }
		},
		"width": {
		  "container": "70rem",
		  "content": "62rem",
		  "lede": "68ch"
		},
		"color": {
		  "border": "#666666",
		  "success": "#457c39",
		  "error": "#db2f27",
		  "disabled": "#999999"
		}
	  }
	},
	"styles": {
	  "blocks": {
		"core/buttons": {
		  "spacing": {
			"blockGap": "1em"
		  }
		}
	  },
	  "color": {
		"background": "#fafcfa",
		"text": "var(--wp--preset--color--dark)"
	  },
	  "elements": {
		"button": {
		  "border": {
			"color": "var(--wp--preset--color--dark)",
			"radius": "8px",
			"style": "solid",
			"width": "2px"
		  },
		  "color": {
			"background": "var(--wp--preset--color--dark)",
			"text": "var(--wp--preset--color--light)"
		  },
		  "spacing": {
			"padding": {
			  "top": "0.8rem",
			  "right": "1.5rem",
			  "bottom": "0.8rem",
			  "left": "1.5rem"
			}
		  },
		  "typography": {
			"fontSize": "var(--wp--preset--font-size--body)",
			"lineHeight": "1",
			"fontWeight": "600"
		  },
		  ":hover": {
				"border": {
					"color": "var(--wp--preset--color--dark)",
					"radius": "8px",
					"width": "2px",
					"style": "solid"
				}
			}
		},
		"h1": {
		  "typography": {
			"fontSize": "var(--wp--preset--font-size--xl)",
			"fontWeight": "800",
			"lineHeight": "var(--wp--custom--typography--line-height--heading)",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
		  }
		},
		"h2": {
		  "typography": {
			"fontSize": "var(--wp--preset--font-size--lg)",
			"fontWeight": "800",
			"lineHeight": "var(--wp--custom--typography--line-height--heading)",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
		  }
		},
		"h3": {
		  "typography": {
			"fontSize": "var(--wp--preset--font-size--md)",
			"fontWeight": "800",
			"lineHeight": "var(--wp--custom--typography--line-height--heading)",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
		  }
		},
		"h4": {
		  "typography": {
			"fontSize": "var(--wp--preset--font-size--body)",
			"fontWeight": "800",
			"lineHeight": "var(--wp--custom--typography--line-height--body)",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
		  }
		},
		"h5": {
		  "typography": {
			"fontSize": "var(--wp--preset--font-size--body)",
			"fontWeight": "800",
			"lineHeight": "var(--wp--custom--typography--line-height--body)",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
		  }
		},
		"h6": {
		  "typography": {
			"fontSize": "var(--wp--preset--font-size--body)",
			"fontWeight": "800",
			"lineHeight": "var(--wp--custom--typography--line-height--body)",
			"letterSpacing": "var(--wp--custom--typography--letter-spacing--heading)"
		  }
		},
		"link": {
		  "color": {
			"text": "var(--wp--preset--color--dark)"
		  }
		}
	  },
	  "spacing": {
		"blockGap": "var(--buffer, var(--wp--custom--spacing--gap))"
	  },
	  "typography": {
		"fontFamily": "var(--wp--preset--font-family--body)",
		"fontSize": "var(--wp--preset--font-size--body)",
		"fontWeight": "600",
		"letterSpacing": "var(--wp--custom--typography--letter-spacing--body)",
		"lineHeight": "var(--wp--custom--typography--line-height--body)"
	  }
	}
  }